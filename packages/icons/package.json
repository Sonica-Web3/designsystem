    "@sonica/eslint-config": "*",
    "@sonica/ts-config": "*",
    "@svgr/babel-plugin-transform-svg-component": "^6.5.1",
    "@svgr/cli": "^6.5.1",
    "@types/react": "^18.0.27",
    "react": "^18.2.0",
    "rollup": "^3.14.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
